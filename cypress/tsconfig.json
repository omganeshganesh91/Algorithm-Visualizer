{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES2015",
    "lib": ["ES2015", "dom"],
    "types": ["cypress", "node"],
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "paths": {
      "@/*": ["../src/*"]
    }
  },
  "include": ["**/*.ts"]
}
